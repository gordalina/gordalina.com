{
  "color": "text",
  "backgroundColor": "codeBackground",
  ".comment,.doc-comment,.prolog,.doctype,.cdata": {
    "color": "#999988"
  },
  ".namespace": {
    "opacity": 0.7
  },
  ".string,.attr-value": {
    "color": "text"
  },
  ".punctuation,.operator": {
    "color": "text"
  },
  ".symbol": {
    "color": "#36acaa"
  },
  ".constant,.attr-name": {
    "color": "#005cc5"
  },
  ".entity,.url,.number,.boolean,.variable,.property,.regex,.inserted": {
    "color": "#36acaa"
  },
  ".atrule,.keyword,.selector,.tag": {
    "color": "#d73a49"
  },
  ".deleted,.tag": {
    "color": "#d73a49"
  },
  ".function-variable": {
    "color": "#6f42c1"
  },
  ".class-name,.function": {
    "color": "#6f42c1"
  },
  ".highlight": {
    "background": "hsla(0, 0%, 70%, .5)"
  }
